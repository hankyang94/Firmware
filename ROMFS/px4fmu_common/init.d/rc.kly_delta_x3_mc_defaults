#!/bin/sh
#
# Multicopter default parameters.
#
# NOTE: Script variables are declared/initialized/unset in the rcS script.
#

set VEHICLE_TYPE mc

if [ $AUTOCNF = yes ]
then
	param set NAV_ACC_RAD 2

	param set RTL_RETURN_ALT 30
	param set RTL_DESCEND_ALT 10
	param set RTL_LAND_DELAY 0

	param set PWM_MAX 2000
	param set PWM_MIN 1000
	param set PWM_RATE 400
fi

#
# This is the gimbal pass mixer.
#
set MIXER_AUX kly_delta_x3_pass

set PWM_AUX_OUT 1234
set PWM_AUX_RATE 50

param set PWM_AUX_DIS1 1000
param set PWM_AUX_DIS2 2000
param set PWM_AUX_DIS3 2000
param set PWM_AUX_DIS4 1000
param set PWM_AUX_FAIL1 1000
param set PWM_AUX_FAIL2 2000
param set PWM_AUX_FAIL3 2000
param set PWM_AUX_FAIL4 1000